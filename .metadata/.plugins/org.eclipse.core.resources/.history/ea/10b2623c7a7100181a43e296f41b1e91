public class Solution {

	int dp []; 
	public int climbStairs(int n) {//总共有多少种走法

		int ret = 0;
		if(n == 1)//只有一种
		{
			return
		}
		
		if(n == 2)
		{
			
		}
		ret = climbStairs(n - 2) + climbStairs(n - 1);
		
		return ret;
        
    }

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
